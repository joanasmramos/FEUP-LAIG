<yas>
    <scene root="root" axis_length="5" />

    <views default="Perspective" >
        <perspective id="Fat Lady Door" near="0.1" far="500" angle="20">
            <from x="3.5" y="1.5" z="20" />
            <to x="3.5" y="1.5" z="0" />
        </perspective>

        <perspective id="Perspective" near="0.1" far="500" angle="20">
            <from x="20" y="20" z="20" />
            <to x="0" y="0" z="0" />
        </perspective>

        <perspective id="Hogwarts History" near="0.1" far="150" angle="10">
            <from x="4" y="5" z="6" />
            <to x="0.5" y="1.2" z="2"/>
        </perspective>

        <ortho id="Orthographic" near="0.1" far="500" left="-20" right="20" top="10" bottom="-10" >
            <from x="20" y="1" z="20" />
            <to x="0" y="0" z="0" />
        </ortho>

    </views>

    <ambient>
        <ambient r="0.4" g="0.4" b="0.4" a="1.0"/>
        <background r="0.075" g="0.125" b="0.150" a="1.0"/>
    </ambient>

    <lights>

        <omni id="General" enabled="1" >
            <location x="3" y="8" z="3" w="1" />
            <ambient r="0.1" g="0.1" b="0.1" a="1" />
            <diffuse r="0.8" g="0.8" b="0.8" a="1" />
            <specular r="0.1" g="0.1" b="0.1" a="1" />
        </omni>

        <omni id="General 2" enabled="1" >
            <location x="6" y="8" z="6" w="1" />
            <ambient r="0.1" g="0.1" b="0.1" a="1" />
            <diffuse r="0.6" g="0.6" b="0.6" a="1" />
            <specular r="0.1" g="0.1" b="0.1" a="1" />
        </omni>

        <spot id="FatLady 1" enabled="0" angle="10" exponent="2" >
            <location x="3.5" y="2" z="0.5" w="1" />
            <target x="3.5" y="3" z="1" />
            <ambient r="0" g="0" b="0" a="1" />
            <diffuse r="1" g="1" b="1" a="1" />
            <specular r="0.5" g="0.5" b="0.5" a="1" />
        </spot>

        <spot id="FatLady 2" enabled="0" angle="20" exponent="2" >
            <location x="7.5" y="2" z="0.5" w="1" />
            <target x="10.5" y="9" z="-5.5" />
            <ambient r="0" g="0" b="0" a="1" />
            <diffuse r="1" g="1" b="1" a="1" />
            <specular r="0.5" g="0.5" b="0.5" a="1" />
        </spot>

        <omni id="Hallway 1" enabled="0" >
            <location x="3.5" y="2" z="-5.5" w="1" />
            <target x="7.5" y="3" z="-5.5" />
            <ambient r="0" g="0" b="0" a="1" />
            <diffuse r="1" g="1" b="1" a="1" />
            <specular r="0.5" g="0.5" b="0.5" a="1" />
        </omni>

        <omni id="Hallway 2" enabled="0" >
            <location x="7.5" y="2" z="-5.5" w="1" />
            <target x="3.5" y="3" z="1" />
            <ambient r="0" g="0" b="0" a="1" />
            <diffuse r="1" g="1" b="1" a="1" />
            <specular r="0.5" g="0.5" b="0.5" a="1" />
        </omni>

    </lights>

    <textures>
        <texture id="fatlady" file="scenes/images/fatlady.jpg" />
        <texture id="brick_walls" file="scenes/images/brick_walls.png" />
        <texture id="history" file="scenes/images/history.jpg" />
        <texture id="wooden_walls" file="scenes/images/wooden_walls.png" />
        <texture id="pillar" file="scenes/images/pilar.png" />
        <texture id="frame" file="scenes/images/frame.png" />
        <texture id="hogwarts_image" file="scenes/images/painting_1.jpg" />
        <texture id="candles_image" file="scenes/images/threeway_painting.jpg" />
        <texture id="structure_2" file="scenes/images/structure_2.png" />
        <texture id="maincarpet" file="scenes/images/main_carpet.png" />
        <texture id="bookshelf_1" file="scenes/images/bookshelf_1.png" />
        <texture id="ceiling" file="scenes/images/floor.jpg" />
        <texture id="floor" file="scenes/images/wooden_floors.png" />
        <texture id="candle" file="scenes/images/candle.jpg" />
        <texture id="metal" file="scenes/images/metal.jpg"/>
        <texture id="terrain" file="scenes/images/terrain.jpg"/>
        <texture id="water" file="scenes/images/water.jpg"/>
        <texture id="heightmap" file="scenes/images/heightmap_terrain.jpg"/>
        <texture id="wavemap" file="scenes/images/heightmap_water.jpg"/>

    </textures>

    <materials>

        <material id="unique_mat" shininess = "1" >
            <emission r="0" g="0" b="0" a="1" />
            <ambient r="0" g="0" b="0" a="1" />
            <diffuse r="0.6" g="0.6" b="0.6" a="1" />
            <specular r="0.1" g="0.1" b="0.1" a="1" />
        </material>

        <material id="metal" shininess="30">
            <emission r="0.2" g="0.2" b="0.2" a="1" />
            <ambient r="0" g="0" b="0" a="1" />
            <diffuse r="0.3" g="0.3" b="0.3" a="1" />
            <specular r="0.9" g="0.9" b="0.9" a="1" />
        </material>

        <material id="metal2" shininess="30">
            <emission r="0" g="0" b="0" a="1" />
            <ambient r="0" g="0" b="0" a="1" />
            <diffuse r="0.3" g="0.3" b="0.3" a="1" />
            <specular r="0.9" g="0.9" b="0.9" a="1" />
        </material>

        <material id="outside" shininess="30">
            <emission r="0.075" g="0.125" b="0.150" a="1.0"/>
            <ambient r="0" g="0" b="0" a="1" />
            <diffuse r="0" g="0" b="0" a="1" />
            <specular r="0" g="0" b="0" a="1" />
        </material>

    </materials>

    <transformations>
        <transformation id="cube_part1">
            <translate x="0" y="0.5" z="0" />
            <rotate axis="x" angle="-90" />
        </transformation>

        <transformation id="cube_part2">
            <translate x="0" y="-0.5" z="0" />
            <rotate axis="x" angle="90" />
        </transformation>

        <transformation id="cube_part3">
            <translate x="0" y="0" z="0.5" />
        </transformation>

        <transformation id="cube_part4">
            <translate x="0" y="0" z="-0.5" />
            <rotate axis="x" angle="-180" />
        </transformation>

        <transformation id="cube_part5">
            <translate x="-0.5" y="0" z="0" />
            <rotate axis="y" angle="-90" />
        </transformation>

        <transformation id="cube_part6">
            <translate x="0.5" y="0" z="0" />
            <rotate axis="y" angle="90" />
        </transformation>

        <transformation id="base_wall">
            <scale x="11" y="8" z="1" />
            <rotate axis="x" angle="90"/>
        </transformation>

        <transformation id="history_wall">
            <translate x="0.01" y="1" z="1.5" />
            <rotate axis="y" angle="90" />
            <rotate axis="x" angle="90" />
            <scale x="1" y="0.8" z="1" />
        </transformation>

        <transformation id="right_wall">
            <translate x="5.5" y="4" z="0" />
        </transformation>

        <transformation id="left_wall">
            <translate x="0" y="4" z="5.5" />
            <rotate axis="y" angle="90" />
        </transformation>

        <transformation id="base_pillar">
            <translate x="0" y="4" z="0" />
            <scale x="1" y="8" z="1" />
        </transformation>

        <transformation id="center_pillar">
          <translate x="0.5" y="0" z="0.5" />
        </transformation>

        <transformation id="right_pillar">
          <translate x="10.5" y="0" z="0.5" />
        </transformation>

        <transformation id="left_pillar">
          <translate x="0.5" y="0" z="10.5" />
        </transformation>

        <transformation id="base_frame">
          <scale x="0.1" y="2.1" z="0.1" />
        </transformation>

        <transformation id="frame1_bkshelf">
          <translate x="0.5" y="1" z="7" />
          <rotate axis="y" angle="90" />
          <scale x="0.9" y="0.9" z="4" />
        </transformation>

        <transformation id="frame2_bkshelf">
          <translate x="0.4" y="2" z="9" />
          <rotate axis="x" angle="-90" />
          <scale x="4" y="2" z="1.5" />
        </transformation>

        <transformation id="left_frame">
          <translate x="4.5" y="1.5" z="0.05" />
          <scale x="1" y="1.5" z="1"/>
        </transformation>

        <transformation id="right_frame">
          <translate x="2" y="0" z="0" />
        </transformation>

        <transformation id="top_frame">
          <translate x="5.5" y="3.05" z="0.05" />
          <rotate axis="z" angle="90" />
        </transformation>

        <transformation id="bottom_frame">
          <translate x="0" y="-3.1" z="0" />
        </transformation>

        <transformation id="hogwarts_frame">
          <translate x="0" y="0" z="-1" />
          <rotate axis="y" angle="-90" />
          <translate x="0.5" y="2.5" z="-0.1" />
          <scale x="0.7" y="1" z="0.9" />
        </transformation>

        <transformation id="hogwarts_image">
          <translate x="0" y="0" z="-1" />
          <translate x="0.1" y="4" z="4.35" />
          <scale x="1" y="3" z="1.3" />
          <rotate axis="y" angle="90"/>
        </transformation>

        <transformation id="candles_frame1">
          <translate x="1.3" y="3.5" z="0.05" />
        </transformation>

        <transformation id="candles_frame2">
          <translate x="2.3" y="3.5" z="0.05" />
        </transformation>

        <transformation id="candles_frame3">
          <translate x="3.3" y="3.5" z="0.05" />
        </transformation>

        <transformation id="fatlady_painting">
          <translate x="5.5" y="1.5" z="0.03" />
          <scale x="2" y="3" z="1" />
        </transformation>

        <transformation id="fatlady_painting2">
          <translate x="5.5" y="1.5" z="0.02" />
          <rotate axis="y" angle="-180"/>
          <scale x="2" y="3" z="1" />
        </transformation>

        <transformation id="wall_hole">
          <translate x="5.5" y="1.55" z="0.02"/>
          <scale x="2" y="3.1" z="1" />
        </transformation>

        <transformation id="candles_image">
          <translate x="7.8" y="5" z="0.1" />
          <scale x="4" y="3" z="0.1" />
        </transformation>

        <transformation id="maincarpet">
          <translate x="5.5" y="0" z="5.5" />
          <rotate axis="x" angle="-90" />
          <scale x="5" y="6" z="0.1" />
        </transformation>

        <transformation id="bookshelf_1">
          <translate x="0.4" y="1.25" z="8.5" />
          <scale x="0.5" y="2.5" z="2.5" />
        </transformation>

        <transformation id="floor">
          <translate x="5.5" y="0" z="5.5" />
          <scale x="11" y="1" z="11" />
        </transformation>

        <transformation id="ceiling">
          <translate x="5.5" y="8" z="5.5" />
          <rotate axis="x" angle="-90"/>
          <rotate axis="y" angle="180"/>
          <scale x="11" y="11" z="1" />
        </transformation>

    </transformations>

    <animations>
        <linear id="linear1" span="5">
            <controlpoint xx="1" yy="0" zz="1" />
            <controlpoint xx="5" yy="0" zz="1" />
            <controlpoint xx="5" yy="0" zz="3" />
        </linear>

        <linear id="linear2" span="5">
            <controlpoint xx="5" yy="0" zz="3" />
            <controlpoint xx="5" yy="4" zz="3" />
            <controlpoint xx="7" yy="0" zz="5" />
        </linear>

        <linear id="snitch1" span="4">
            <controlpoint xx="5" yy="2" zz="-5"/>
            <controlpoint xx="5" yy="2" zz="2"/>
            <controlpoint xx="6" yy="1" zz="2"/>
            <controlpoint xx="6" yy="1" zz="3"/>
        </linear>

        <linear id="snitch3" span="0.5">
            <controlpoint xx="2" yy="1" zz="3"/>
            <controlpoint xx="3" yy="1" zz="3"/>
        </linear>

        <circular id="snitch2" span="3" center="4 1 3" radius="2" startang="0" rotang="-180" />

        <circular id="door" span="8" center="0 0 0" radius="0.01" startang="0" rotang="-89" />

    </animations>

    <primitives>
        <primitive id="rectangle">
            <rectangle x1="-0.5" y1="-0.5" x2="0.5" y2="0.5" />
        </primitive >
        <primitive id="triangle">
            <triangle x1="0" y1="1" z1="0" x2="-0.5" y2="0" z2="0" x3="0.5" y3="0" z3="0"/>
        </primitive>
        <primitive id="cylinder">
            <cylinder base="1" top="1" height="1" stacks="100" slices="100"/>
        </primitive>
        <primitive id="sphere">
            <sphere radius="1" stacks="32" slices="64"/>
        </primitive>
        <primitive id="torus">
            <torus inner="1" outer="2" slices="64" loops="32"/>
        </primitive>
        <primitive id="plane">
            <plane npartsU="10" npartsV="10"/>
        </primitive>
        <primitive id="patch">
            <patch npointsU="3" npointsV="2" npartsU="10" npartsV="10" >
                <controlpoint xx="-1.5" yy="-1.5" zz="0" />
                <controlpoint xx="-1.5" yy="1.5" zz="0" />
                <controlpoint xx="0" yy="-1.5" zz="3" />
                <controlpoint xx="0" yy="1.5" zz="3" />
                <controlpoint xx="1.5" yy="-1.5" zz="0" />
                <controlpoint xx="1.5" yy="1.5" zz="0" />
            </patch>
        </primitive>
        <primitive id="cylinder2">
            <cylinder2 base="1" top="1" height="1" slices="10" stacks="10" />
        </primitive>
        <primitive id="vehicle">
              <vehicle/>
        </primitive>
        <primitive id="terrain">
            <terrain idtexture="terrain" idheightmap="heightmap" parts="10" heightscale="4"/>
        </primitive>
        <primitive id="water">
            <water idtexture="water" idwavemap="wavemap" parts="50" heightscale="0.2" texscale="1" />
        </primitive>
    </primitives>

    <components>
        <component id="root">
            <transformation>
            </transformation>
            <materials>
                <material id="unique_mat" />
            </materials>
            <texture id="none" />
            <children>
                <componentref id="snitch"/>
                <componentref id="water_test"/>
                <componentref id="terrain_test"/>
                <componentref id="terrain_water_support"/>
                <componentref id="history_hogwarts"/>
                <componentref id="fatlady_door"/>
                <componentref id="bookshelf_1" />
                <componentref id="ceiling"/>
                <componentref id="floor"/>
                <componentref id="right_wall" />
                <componentref id="left_wall" />
                <componentref id="center_pillar" />
                <componentref id="right_pillar" />
                <componentref id="left_pillar" />
                <componentref id="hogwarts_painting"/>
                <componentref id="candles_painting"/>
                <componentref id="chandelier2"/>
                <componentref id="chandelier1"/>
                <componentref id="wall_hole"/>
            </children>
        </component>

        <component id="snitch">
            <transformation>
            </transformation>
            <animations>
              <animationref id ="snitch1"/>
              <animationref id ="snitch2"/>
              <animationref id ="snitch3"/>
            </animations>
            <materials>
                <material id="inherit"/>
            </materials>
            <texture id="none" />
            <children>
                <primitiveref id="vehicle"/>
            </children>
        </component>

        <component id="water_test">
            <transformation>
                <translate x="0.5" y="1.6" z="2.99"/>
                <scale x="0.40" y="0.10" z="0.40"/>
            </transformation>
            <animations>
            </animations>
            <materials>
                <material id="inherit"/>
            </materials>
            <texture id="none" />
            <children>
                <primitiveref id="water"/>
            </children>
        </component>

        <component id="terrain_test">
            <transformation>
                <translate x="0.5" y="1.45" z="3"/>
                <scale x="0.5" y="0.1" z="0.5"/>
            </transformation>
            <animations>
            </animations>
            <materials>
                <material id="inherit"/>
            </materials>
            <texture id="none" />
            <children>
                <primitiveref id="terrain"/>
            </children>
        </component>

        <component id="terrain_water_support">
            <transformation>
            <translate x="-5.5" y="0" z="2" />
            </transformation>
            <materials>
                <material id="metal"/>
            </materials>
            <texture id="metal" length_s="5.0" length_t="5.0"/>
            <children>
                <componentref id="chandelier_base"/>
                <componentref id="chandelier_bigbase-0"/>
                <componentref id="chandelier_bigbase-3"/>
            </children>
        </component>

        <component id="history_hogwarts">
            <transformation>
              <transformationref id="history_wall"/>
            </transformation>
            <materials>
                    <material id="inherit" />
            </materials>
            <texture id="history" length_s="1" length_t="1" />
            <children>
                <primitiveref id="plane" />
            </children>
        </component>

        <component id="fatlady_door">
            <transformation>
                <translate x="4.5" y="0" z="0" />
                <translate x="-4.5" y="0" z="0" />
            </transformation>
            <animations>
              <animationref id="door"/>
            </animations>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="none" length_s="1" length_t="1" />
            <children>
                <componentref id="fatlady_painting"/>
                <componentref id="fatlady_painting2"/>
                <componentref id="top_frame"/>
                <componentref id="bottom_frame"/>
                <componentref id="right_frame"/>
                <componentref id="left_frame"/>
            </children>
        </component>

        <component id="wall_hole">
            <transformation>
                <transformationref id="wall_hole" />
            </transformation>
            <materials>
                <material id="outside" />
            </materials>
            <texture id="none" />
            <children>
                <primitiveref id="rectangle" />
            </children>
        </component>

        <component id="fatlady_painting2">
            <transformation>
                <transformationref id="fatlady_painting2" />
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="fatlady" length_s="1" length_t="1" />
            <children>
                <primitiveref id="rectangle" />
            </children>
        </component>

        <component id="fatlady_painting">
            <transformation>
                <transformationref id="fatlady_painting" />
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="fatlady" length_s="1" length_t="1" />
            <children>
                <primitiveref id="rectangle" />
            </children>
        </component>

        <component id="bottom_frame">
            <transformation>
                <transformationref id="bottom_frame" />
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="none" length_s="1" length_t="1" />
            <children>
                <componentref id="top_frame" />
            </children>
        </component>

        <component id="top_frame">
            <transformation>
                <transformationref id="top_frame" />
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="none" length_s="1" length_t="1" />
            <children>
                <componentref id="base_frame" />
            </children>
        </component>

        <component id="right_frame">
            <transformation>
                <transformationref id="right_frame" />
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="none" length_s="1" length_t="1" />
            <children>
                <componentref id="left_frame" />
            </children>
        </component>

        <component id="left_frame">
            <transformation>
                <transformationref id="left_frame" />
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="none" length_s="1" length_t="1" />
            <children>
                <componentref id="base_frame" />
            </children>
        </component>

        <component id="painting_frame">
            <transformation>
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="none" length_s="1" length_t="1" />
            <children>
                <componentref id="left_frame"/>
                <componentref id="right_frame"/>
                <componentref id="top_frame"/>
                <componentref id="bottom_frame"/>
            </children>
        </component>

        <component id="hogwarts_painting">
            <transformation>
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="none" length_s="1" length_t="1" />
            <children>
                <componentref id="hogwarts_image"/>
                <componentref id="hogwarts_frame"/>
            </children>
        </component>

        <component id="hogwarts_frame">
            <transformation>
                <transformationref id="hogwarts_frame" />
            </transformation>
            <materials>
                <material id="unique_mat"/>
            </materials>
            <texture id="frame" length_s="5.0" length_t="5.0"/>
            <children>
                <componentref id="painting_frame"/>
            </children>
        </component>

        <component id="hogwarts_image">
            <transformation>
                <transformationref id="hogwarts_image" />
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="hogwarts_image" length_s="1" length_t="1" />
            <children>
                <primitiveref id="rectangle" />
            </children>
        </component>

        <component id="candles_painting">
            <transformation>
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="none" length_s="1" length_t="1" />
            <children>
                <componentref id="candles_frame1" />
                <componentref id="candles_frame2" />
                <componentref id="candles_frame3" />
                <componentref id="candles_image" />
            </children>
        </component>

        <component id="candles_frame1">
            <transformation>
                <transformationref id="candles_frame1" />
            </transformation>
            <materials>
                <material id="unique_mat"/>
            </materials>
