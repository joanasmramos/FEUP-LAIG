<yas>
    <scene root="unique_comp" axis_length="5" />

    <views default="unique_persp" >

        <perspective id="unique_persp" near="0.1" far="500" angle="0.4">
            <from x="15" y="15" z="15" />
            <to x="0" y="0" z="0" />
        </perspective>

    </views>

    <ambient>
        <ambient r="0.1" g="0.1" b="0.1" a="1" />
        <background r="0.76" g="1" b="0.85" a="1" />
    </ambient>

    <lights>

        <omni id="unique_omni" enabled="1" >
            <location x="2" y="2" z="0" w="1" />
            <ambient r="0.1" g="0.1" b="0.1" a="1" />
            <diffuse r="0.8" g="0.8" b="0.8" a="1" />
            <specular r="0.1" g="0.1" b="0.1" a="1" />
        </omni>

    </lights>

    <textures>
        <texture id="unique_tex" file="images/trunk-image.jpg" />
    </textures>

    <materials>
        <material id="unique_mat" shininess = "1" >
            <emission r="0" g="0" b="0" a="1" />
            <ambient r="0" g="0" b="0" a="1" />
            <diffuse r="0.6" g="0.6" b="0.6" a="1" />
            <specular r="0.3" g="0.3" b="0.3" a="1" />
        </material>
    </materials>

    <transformations>
        <transformation id="unique_trf">
            <translate x="0" y="0" z="0" />
        </transformation>
    </transformations>

    <primitives>
        <primitive id="unique_prim">
            <rectangle x1="0" y1="0" x2="5" y2="5" />
        </primitive >
    </primitives >

 <components>
    <component id="unique_comp">
        <transformation>
            <transformationref id="unique_trf" />
        </transformation>
        <materials>
            <material id="unique_mat" />
        </materials>
        <texture id="unique_tex" length_s="1" length_t="1" />
        <children>
            <primitiveref id="unique_prim" />
        </children>
    </component>
 </components>

</yas>
